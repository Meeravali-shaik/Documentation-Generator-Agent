{
  "version": 2,
  "builds": [
    {
      "src": "webapp.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webapp.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": {
      "description": "Google Gemini API Key for documentation generation"
    }
  }
}
